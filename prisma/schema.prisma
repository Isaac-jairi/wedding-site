generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Gift {
  id          String  @id @default(cuid())
  name        String
  description String
  price       Float
  imageUrl    String
  url         String?
  isReserved  Boolean @default(false)
  reservedBy  String?
}